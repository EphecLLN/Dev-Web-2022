{
  "name": "dev-web-2022",
  "version": "0.0.1",
  "private": true,
  "description": "Website development project for the Dev-3 course at EPHEC college",
  "license": "GPLv3",
  "scripts": {
    "build": "cross-env NODE_ENV=development && webpack -c webpack.config.dev.js",
    "dev": "(npm run watch &) && npm run serve",
    "serve": "cross-env NODE_ENV=development && node server/main.js",
    "integration": "jest --config tests/integration/jest.config.js",
    "lint": "eslint .",
    "unit": "jest --config tests/unit/jest.config.js",
    "validation": "cucumber-js -c ./cucumber.js tests/validation",
    "watch": "cross-env NODE_ENV=development && webpack -c webpack.config.dev.js --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
